struct StringIterator: Copyable, Iterator<char> {
    char[?]* current;
    char[?]* end;

    init(string s) {
        current = s.data();
        end = &s.data()[s.size()];
    }

    bool hasValue() {
        return current !== end;
    }

    char value() {
        return *current;
    }

    void increment() {
        current++;
    }
}

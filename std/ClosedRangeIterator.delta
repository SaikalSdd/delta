struct ClosedRangeIterator<T>: Copyable, Iterator<T> {
    T current;
    T end;

    init(ClosedRange<T> range) {
        current = range.start();
        end = range.end();
    }

    bool hasValue() {
        return current <= end;
    }

    T value() {
        return current;
    }

    mutating void increment() {
        current++;
    }
}

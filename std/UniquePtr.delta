struct UniquePtr<Pointee> {
	mutable Pointee* pointer;

	init(Pointee value) {
		this.pointer = allocate(value);
	}

	init(mutable Pointee* pointer) {
		this.pointer = pointer;
	}

	deinit() {
		deallocate(pointer);
	}

	Pointee* get() {
		return pointer;
	}

	mutating mutable Pointee* get() {
		return pointer;
	}
}

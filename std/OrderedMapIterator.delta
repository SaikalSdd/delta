struct OrderedMapIterator<Key, Value>: Copyable, Iterator<(Key key, mutable Value value)*> {
    AVLnode<Key, Value>*? current;
    OrderedMap<Key, Value>* map;

    init(OrderedMap<Key, Value>* map) {
        current = map.firstNode();
        this.map = map;
    }

    bool hasValue() {
        return current != null;
    }

    (Key key, mutable Value value)* value() {
        return current!.entry;
    }

    mutating void increment() {
        current = map.higherNode(current!);
    }
}

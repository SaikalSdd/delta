// RUN: check_exit_status 0 %delta run -Werror %s

void main() {
    testCustomPrintableStruct();
}

struct A: Printable {
    StringRef a;
    StringRef b;

    init(StringRef a, StringRef b) {
        this.a = a;
        this.b = b;
    }

    void printTo(String* stream) {
        stream.write(a);
        stream.write("+");
        stream.write(b);
    }
}

void testCustomPrintableStruct() {
    assert(StringRef(A("foo", "bar").toString()) == "foo+bar");
}

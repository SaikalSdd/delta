// RUN: %delta run -Werror %s | %FileCheck -match-full-lines %s

struct A: Printable {
    StringRef a;

    void print(String* stream) {
        stream.write(a);
        stream.write(a);
        stream.write("!");
    }
}

void main() {
    print(true); // CHECK: true
    print(false); // CHECK-NEXT: false
    print(A("foo")); // CHECK-NEXT: foofoo!
    print(0.1 + 0.2); // CHECK-NEXT: 0.3
}

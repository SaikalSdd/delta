// RUN: check_exit_status 0 compile_and_run -Werror %s

struct C: Comparable {

    int a;

    init(int a) {
        this.a = a;
    }

    Ordering compare(C* other) {
        return this.a.compare(other.a);
    }
}

void main() {
    var a = C(6);
    var b = C(99);

    assert(a <= b);
    assert(!(a >= b));
    assert(!(a > b));
    assert(a < b);
    assert(!(a == b));
    assert(a != b);

    assert(&a <= &b);
    assert(!(&a >= &b));
    assert(!(&a > &b));
    assert(&a < &b);
    assert(!(&a == &b));
    assert(&a != &b);
}

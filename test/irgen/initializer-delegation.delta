// RUN: %delta -print-ir -Werror %s | %FileCheck %s -check-prefix=CHECK-0
// RUN: %delta -print-ir -Werror %s | %FileCheck %s -check-prefix=CHECK-1
// RUN: %delta -print-ir -Werror %s | %FileCheck %s -check-prefix=CHECK-2

func main() {
    let x = X(4, 2);
}

struct X {
    // CHECK-2: define void @X.init{{[\.0-9]*}}({}* %this, i32 %a, i32 %b)
    // CHECK-2-NEXT: call void @X.init{{[\.0-9]*}}({}* %this, i32 %a)
    // CHECK-2-NEXT: ret void
    init(a: int, b: int) {
        init(a);
    }

    // CHECK-1: define void @X.init{{[\.0-9]*}}({}* %this, i32 %a)
    // CHECK-1-NEXT: call void @X.init{{[\.0-9]*}}({}* %this)
    // CHECK-1-NEXT: ret void
    init(a: int) {
        init();
    }

    // CHECK-0: define void @X.init{{[\.0-9]*}}({}* %this)
    // CHECK-0-NEXT: ret void
    init() {}
}

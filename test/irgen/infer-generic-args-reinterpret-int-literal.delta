// RUN: %delta -print-ir %s | %FileCheck %s
// RUN: %delta -print-ir %s | %FileCheck %s -check-prefix=CHECK-F

// CHECK: define void @{{.*g.*}}(i32 %u)
def g(u: uint) {
    // CHECK-NEXT: call void @{{.*f.*uint.*}}(i32 0, i32 %u)
    f(0, u);
}

// CHECK-F: define void @{{.*f.*uint.*}}(i32 %a, i32 %b)
def f<T>(a: T, b: T) {}

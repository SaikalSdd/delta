// RUN: %delta -print-ir %s | %FileCheck %s

class X { let i: int; }
class Y { let i: int; }

// CHECK: define i1 [[EQ:@"<\.[0-9]+"]](%X* %a, %X* %b)
func <(a: X*, b: X*) -> bool {
    return true;
}

interface Z {
    func z();
}

func < <T: Z>(a: T*, b: T*) -> bool {
    return true;
}

// CHECK: define void @fx(%X* %a, %X* %b)
// CHECK-NEXT: call i1 [[EQ]](%X* %a, %X* %b)
// CHECK-NEXT: call i1 [[EQ]](%X* %a, %X* %b)
func fx(a: X*, b: X*) {
    _ = a < b;
    _ = *a < *b;
}

// CHECK: define void @fy(%Y* %a, %Y* %b)
// CHECK-NEXT: %1 = bitcast %Y* %a to i8*
// CHECK-NEXT: %2 = bitcast %Y* %b to i8*
// CHECK-NEXT: %3 = icmp ult i8* %1, %2
// CHECK-NEXT: ret void
func fy(a: Y*, b: Y*) {
    _ = cast<void*>(a) < cast<void*>(b);
}

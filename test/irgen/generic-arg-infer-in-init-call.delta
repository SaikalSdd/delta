// RUN: %delta -print-ir %s | %FileCheck %s
// RUN: %delta -print-ir %s | %FileCheck %s -check-prefix=CHECK-CALL

struct C<T> {
    let t: T;

    // CHECK: define void @"C<int>.init"(%"C<int>"* %this, i32 %t)
    // CHECK: define void @"C<StringRef>.init"(%"C<StringRef>"* %this, %StringRef %t)
    init(t: T) {}
}

func main() {
    // CHECK-CALL: call void @"C<int>.init"(%"C<int>"* %i, i32 42)
    let i = C(42);
    // CHECK-CALL: call void @"C<StringRef>.init"(%"C<StringRef>"* %b, %StringRef %__str{{[0-9]+}})
    let b = C("");
}

// RUN: %delta -print-ir %s | %FileCheck %s

struct Foo { var i: int; }
def main() {
    // CHECK: %f = alloca %Foo
    // CHECK-NEXT: %rf = alloca %Foo*
    // CHECK-NEXT: %pf = alloca %Foo*
    var f: Foo = undefined;
    var rf: Foo* = f; // CHECK-NEXT: store %Foo* %f, %Foo** %rf
    var pf: Foo* = f; // CHECK-NEXT: store %Foo* %f, %Foo** %pf
}

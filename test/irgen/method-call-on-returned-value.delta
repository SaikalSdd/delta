// RUN: check_matches_snapshot %s.ll %delta -print-ir %s

struct S: Copyable {
    var i: int;
    init() { i = 0; }
    def f() {}
}

struct C {
    var i: int;
    init() { i = 0; }
    def f() {}
}

def s(): S { return S(); }
def c(): C { return C(); }

def main() {
    s().f();
    c().f();
    S().f();
    C().f();
}

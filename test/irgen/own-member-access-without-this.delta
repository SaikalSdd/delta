// RUN: check_matches_snapshot %s.ll %delta -print-ir %s -w

struct A {
    var i: int
    var j: int

    init() {
        j = 42
    }

    def foo() {
        _ = j
    }

    mutating def bar() {
        j = 1
    }
}

struct B: Copyable {
    var i: int
    var j: int

    init() {
        j = 42
    }

    def foo() {
        _ = j
    }

    mutating def bar() {
        j = 1
    }
}

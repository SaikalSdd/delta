// RUN: check_matches_snapshot %s.ll %delta -print-ir %s -w

struct A {
    int i
    int j

    init() {
        j = 42
    }

    void foo() {
        _ = j
    }

    mutating void bar() {
        j = 1
    }
}

struct B: Copyable {
    int i
    int j

    init() {
        j = 42
    }

    void foo() {
        _ = j
    }

    mutating void bar() {
        j = 1
    }
}

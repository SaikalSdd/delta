// RUN: %delta -print-ir %s | %FileCheck %s

struct Foo {
    let i: int

    // CHECK: define void @Foo.init(%Foo* %this, i32 %i)
    init(i: int) { }

    // CHECK: define void @Foo.init.{{[0-9]+}}(%Foo* %this, i32 %qux)
    init(qux: int) { }

    // CHECK: define void @Foo.foo(%Foo* %this, i32 %i)
    func foo(i: int) { }

    // CHECK: define void @Foo.foo.{{[0-9]+}}(%Foo* %this, i32 %qux)
    func foo(qux: int) { }
}

// CHECK: define void @foo(i32 %i)
func foo(i: int) { }

// CHECK: define void @foo.{{[0-9]+}}(i32 %qux)
func foo(qux: int) { }

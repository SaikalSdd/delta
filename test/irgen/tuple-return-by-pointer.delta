// RUN: %delta -print-ir %s | %FileCheck %s

class S {
    var t: (a: int);

    // CHECK: define { i32 }* @S.f(%S* %this)
    func f() -> (a: int)* {
        // CHECK-NEXT: %1 = getelementptr inbounds %S, %S* %this, i32 0, i32 0
        // CHECK-NEXT: ret { i32 }* %1
        return &t;
    }

    // CHECK: define { i32 }* @S.g(%S* %this)
    func g() -> (a: int)* {
        // CHECK-NEXT: %1 = getelementptr inbounds %S, %S* %this, i32 0, i32 0
        // CHECK-NEXT: ret { i32 }* %1
        return t;
    }
}

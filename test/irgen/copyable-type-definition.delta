// RUN: check_matches_snapshot %s.ll %delta -print-ir %s

struct Foo: Copyable {
    var a: int;
    var b: bool;

    init(a: int, b: bool) {
        this.a = a;
        this.b = b;
        this.a++;
    }
}

def main() {
    var f = Foo(666, true);
    f.a++;
    var bar = f.a;
    var qux = f.b;
}

// RUN: check_matches_snapshot %s.ll %delta -print-ir %s

struct S: Copyable {
    init() {}

    mutating def foo() {
        bar()
    }

    def bar() { }
}

struct Foo<U> {
    init() {
        baz()
    }

    deinit() {
        baz()
    }

    mutating def baz() {
        qux()
    }

    def qux() { }
}

def main() {
    var s = S()
    s.foo()

    var t = Foo<int>()
    t.baz()
}

// RUN: check_matches_snapshot %s.ll %delta -print-ir %s

struct S: Copyable {
    init() {}

    void foo() {
        bar()
    }

    void bar() { }
}

struct Foo<U> {
    init() {
        baz()
    }

    deinit() {
        baz()
    }

    void baz() {
        qux()
    }

    void qux() { }
}

void main() {
    var s = S()
    s.foo()

    var t = Foo<int>()
    t.baz()
}

// RUN: check_matches_snapshot %s.ll %delta -print-ir %s

def foo<T>(t: T) { }

def bar<T>(t: T): T { return t; }

def qux<T>(t: T): T {
    if (t < 0) { return -t; }
    return t;
}

def main() {
    foo<int>(1);
    foo<bool>(false);
    foo<bool>(true);
    var b = bar("bar");
    var five = qux(-5);
}

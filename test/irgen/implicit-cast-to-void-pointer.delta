// RUN: check_matches_snapshot %s.ll %delta -print-ir %s

def main() {
    var p: int* = undefined;
    var mp: mutable int* = undefined;
    var v: void* = p;
    bar(p);
    bar(v);
    var b = cast<int8*>(v);
    v = b;
    baz(mp);
    var i = 0;
    baz(&i);
}

def bar(p: void*) {}

def baz(p: void*?) {}

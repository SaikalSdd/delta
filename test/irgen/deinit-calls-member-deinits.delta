// RUN: check_matches_snapshot %s.ll %delta -print-ir %s

extern bool f();

struct A {
    B b;
    B bb;
    int i;

    init() {
        b = undefined;
        bb = undefined;
        i = undefined;
    }

    deinit() {
        if (f()) {
            return;
        }
    }
}

struct B {
    int i;
    init() { i = 0; }
    deinit() {}
}

struct C {
    B b;
    B bb;
    init() {
        b = undefined;
        bb = undefined;
    }
}

void main() {
    var a = A();
    var c = C();
}

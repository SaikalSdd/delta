// RUN: check_matches_snapshot %s.ll %delta -print-ir %s

var x = 0;

enum A {
    case x;
    case y;
    case z;
}

def main() {
    var foo = A.z;
    if (foo == A.y) {}
    if (foo != A.x) {}
    switch (foo) {
        case A.x: return;
        case A.y: return;
        case A.z: return;
    }
}

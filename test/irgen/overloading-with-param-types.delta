// RUN: check_matches_snapshot %s.ll %delta -print-ir %s

struct S: Copyable {
    int i;
    init() { i = 0; }
}

struct T: Copyable {
    int i;
    init() { i = 0; }
}

void f(S a) {}
void f(T a) {}
void operator+(S a, S b) {}
void operator+(T a, T b) {}

void main() {
    var s = S();
    var t = T();
    f(s);
    f(t);
    _ = s + s;
    _ = t + t;
    _ = 1 + 1;
}

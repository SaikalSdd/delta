// RUN: check_matches_snapshot %s.ll %delta -print-ir %s

void foo(int i) {
    switch (i) {
        case 0:
            break;
        case -1:
            foo(i);
            foo(i);
        case 42:
            break;
    }
    switch (i) {
        case 0:
            break;
        default:
            foo(i);
    }
}

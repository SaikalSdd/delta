// RUN: %delta -print-ir %s | %FileCheck %s

struct Foo {
    let i: int;
    func foo() { }
}

struct Bar: Copyable {
    let i: int;
    func bar() { }
}

func qux(f: Foo*, b: Bar*) {
    // CHECK: call void @Foo.foo(%Foo* %f)
    f.foo();
    // CHECK-NEXT: %1 = load %Bar, %Bar* %b
    // CHECK-NEXT: call void @Bar.bar(%Bar %1)
    b.bar();
}

// RUN: %delta -print-ir %s | %FileCheck %s

struct Foo {
    var i: int;
    def foo() { }
}

struct Bar: Copyable {
    var i: int;
    def bar() { }
}

def qux(f: Foo*, b: Bar*) {
    // CHECK: call void @_EN4main3Foo3fooE(%Foo* %f)
    f.foo();
    // CHECK-NEXT: %1 = load %Bar, %Bar* %b
    // CHECK-NEXT: call void @_EN4main3Bar3barE(%Bar %1)
    b.bar();
}

// RUN: check_matches_snapshot %s.ll %delta -print-ir %s

struct Foo {
    var i: int;
    init() { i = 0; }
    def foo() { }
}

struct Bar: Copyable {
    var i: int;
    init() { i = 0; }
    def bar() { }
}

def qux(f: Foo*, b: Bar*) {
    f.foo();
    b.bar();
}

def main() {
    var f = Foo();
    var b = Bar();
    qux(f, b);
}

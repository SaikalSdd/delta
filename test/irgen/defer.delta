// RUN: check_matches_snapshot %s.ll %delta -print-ir %s

extern def foo();
extern def bar();

def main() {
    defer foo();
    if (false) {
        defer bar();
        return;
    }
    if (false) {
        defer bar();
    }
    while (false) {
        defer foo();
        bar();
    }
    defer bar();
    if (true) {
        return;
    }
    foo();
    shouldBeEmpty();
}

def shouldBeEmpty() { }

// RUN: not %delta -typecheck %s | %FileCheck %s

struct S: Copyable {
    var a: mutable S*?;
}

def foo(p: mutable S*) -> mutable S*? {
    var n = p;

    while (n.a != null) {
        // CHECK: [[@LINE+1]]:22: error: cannot unwrap non-optional type 'mutable S*'
        if (n === n.a!.a) {
            return n.a!.a;
        }
        n = n.a;
    }

    return null;
}

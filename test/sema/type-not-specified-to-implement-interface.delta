// RUN: not %delta -typecheck %s | %FileCheck %s

interface X {
    def foo();
    def bar();
    def qux();
}

struct Y {
    init() {}
    def foo() {}
    def bar() {}
    def qux() {}
}

def f<T: X>(t: T) {}

def main() {
    // CHECK: [[@LINE+1]]:6: error: type 'Y' doesn't implement interface 'X'
    f(Y());
}
